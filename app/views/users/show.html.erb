<h1>マイページ</h1>
<% if user_signed_in? %>
  <%= image_tag current_user.image.url %><br>
  <%= current_user.name %><br>
  <%= current_user.introduction %><br>
  <%= link_to 'アカウント編集画面へ', edit_user_registration_path %><br>
  <% @liked_posts.each do |likes| %>
    <table style="border: 1px solid black ;">
      <td><%= image_tag likes.image.url %></td>
      <td><%= likes.content %></td>
      <td><%= likes.user.name %></td>
      <br>
    </table>
  <% end %>
  <%= link_to '確認用のコレクション', collections_path %>
  <%= paginate @liked_posts %>
<% end %>
